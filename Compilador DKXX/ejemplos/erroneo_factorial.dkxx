declare{
 
    const int hola = 4;
    
    function int factorial(int num){
        bool posible = hola; #error de tipo
        if (num != 1){
            posible=true;
            int aux=num;
            num = num - 1;
            while(posible){
                aux = aux * num;
                num = num - 1;
            }
            return aux;
        }
        return num;
    }
}

main{
    int i = input();
    output(factorial(i))); # error de sintaxis por parentesis extra
}